    <RelativeLayout android:id="@+id/layout_broadcast"
        style="@style/RelativeLayout_interface_element_1">

        <FrameLayout
            android:id="@+id/frame_label_broadcast"
            style="@style/frame_label_interface_element_1">

            <TextView
                android:text="@string/txt_label_broadcastIP"
                android:id="@+id/label_broadcastIP"
                style="@style/label_inerface_element_1" />
        </FrameLayout>

        <TextView
            android:text="255"
            android:id="@+id/txt_IP_broadcast_1"
            style="@style/txt_IPAddr_element_1.column1"
            android:layout_below="@+id/frame_label_broadcast"
