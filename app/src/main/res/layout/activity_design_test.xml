        <TextView
            android:text="00000000"
            android:id="@+id/label_IPbin_firstHost_4"
            android:layout_below="@+id/txt_IP_firstHost_4"
            android:layout_alignLeft="@+id/txt_IP_firstHost_4"
            android:textIsSelectable="true"
            style="@style/label_IPbin_1" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout_lastHost"
        style="@style/RelativeLayout_interface_element_1">

        <FrameLayout
            android:id="@+id/frame_label_lastHostIP"
            style="@style/frame_label_interface_element_1">

            <TextView
