<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <LinearLayout
        style="?android:attr/buttonBarStyle"
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal" >

        <Button
            style="?android:attr/buttonBarButtonStyle"
            android:id="@+id/button"
            android:layout_width="0px"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="@string/btn_subnets"
            android:textSize="14sp"
            android:layout_gravity="center_vertical"
            android:background="#BBBBBB" />

        <Button
            style="?android:attr/buttonBarButtonStyle"
            android:id="@+id/button2"
            android:layout_width="0px"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="@string/btn_hostLookup"
            android:textSize="14sp"
            android:layout_gravity="center_vertical"
            android:background="#BBBBBB" />

        <Button
            style="?android:attr/buttonBarButtonStyle"
            android:id="@+id/button3"
            android:layout_width="0px"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="@string/btn_subnetLookup"
            android:textSize="14sp"
            android:layout_gravity="center_vertical"
            android:clickable="false"
            android:textColor="#BBBBBB"
            android:background="#555555" />
    </LinearLayout>

    <RelativeLayout
        style="@style/RelativeLayout_interface_element_1"
        android:id="@+id/layout_subnetIndex">

        <FrameLayout
            android:id="@+id/frame_label_subnetIndex"
            style="@style/frame_label_interface_element_1">

            <TextView
                android:text="@string/txt_label_subnetIndex"
                android:id="@+id/label_subnetIndex"
                style="@style/label_inerface_element_1" />

        </FrameLayout>

        <EditText
            android:layout_width="130dp"
            android:id="@+id/txt_subnetIndex"
            android:layout_alignStart="@id/frame_label_subnetIndex"
            android:layout_below="@+id/frame_label_subnetIndex"
            android:text="62225"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginTop="10dp"
            style="@style/txt_main_textbox" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout_subnetAddr"
        style="@style/RelativeLayout_interface_element_1">

        <FrameLayout
            android:id="@+id/frame_label_subnetAddrIP"
            style="@style/frame_label_interface_element_1">

            <TextView
                android:text="@string/txt_label_subnetIP"
                android:id="@+id/label_subnetAddressIP"
                style="@style/label_inerface_element_1" />
        </FrameLayout>

        <TextView
            android:text="255"
            android:id="@+id/txt_IP_subnetAddr_1"
            style="@style/txt_IPAddr_element_1.column1"
            android:layout_below="@+id/frame_label_subnetAddrIP"
            android:layout_alignStart="@id/frame_label_subnetAddrIP" />

        <TextView
            android:text="255"
            android:id="@+id/txt_IP_subnetAddr_2"
            style="@style/txt_IPAddr_element_1"
            android:layout_below="@+id/frame_label_subnetAddrIP"
            android:layout_toRightOf="@+id/txt_IP_subnetAddr_1" />

        <TextView
            android:text="255"
            android:id="@+id/txt_IP_subnetAddr_3"
            style="@style/txt_IPAddr_element_1"
            android:layout_below="@+id/frame_label_subnetAddrIP"
            android:layout_toRightOf="@+id/txt_IP_subnetAddr_2" />

        <TextView
            android:text="255"
            android:id="@+id/txt_IP_subnetAddr_4"
            style="@style/txt_IPAddr_element_1"
            android:layout_below="@+id/frame_label_subnetAddrIP"
            android:layout_toRightOf="@+id/txt_IP_subnetAddr_3" />

        <TextView
            android:text="00000000"
            android:id="@+id/label_IPbin_subnetAddr_1"
            android:layout_below="@+id/txt_IP_subnetAddr_1"
            android:layout_alignLeft="@+id/txt_IP_subnetAddr_1"
            style="@style/label_IPbin_1" />

        <TextView
            android:text="00000000"
            android:id="@+id/label_IPbin_subnetAddr_2"
            android:layout_below="@+id/txt_IP_subnetAddr_2"
            android:layout_alignLeft="@+id/txt_IP_subnetAddr_2"
            style="@style/label_IPbin_1" />

        <TextView
            android:text="00000000"
            android:id="@+id/label_IPbin_subnetAddr_3"
            android:layout_below="@+id/txt_IP_subnetAddr_3"
            android:layout_alignLeft="@+id/txt_IP_subnetAddr_3"
            style="@style/label_IPbin_1" />

        <TextView
            android:text="00000000"
            android:id="@+id/label_IPbin_subnetAddr_4"
            android:layout_below="@+id/txt_IP_subnetAddr_4"
            android:layout_alignLeft="@+id/txt_IP_subnetAddr_4"
            android:textIsSelectable="true"
            style="@style/label_IPbin_1" />

    </RelativeLayout>

    <RelativeLayout
